<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.barscanv01">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />

    <application
        android:name=".MyApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">
        <activity android:name=".Login.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Operation.OperationSelectActivity" />
        <activity android:name=".SaleLoad.DeliveryBillActivity" />
        <activity android:name=".SaleLoad.SaleLoadActivity" />
        <activity android:name=".SaleLoad.DeliveryBillNoDetailActivity" />
        <activity android:name=".Unload.InOrderBillActivity" />
        <activity android:name=".Unload.UnLoadActivity" />
        <activity android:name=".Translate.TranslateActivity" />
        <activity android:name=".Unload.NoDetailUnLoadActivity" />
        <activity android:name=".Setting.SettingActivity" />
        <activity android:name=".SaleLoad.GoodAddDetailActivity" />
        <activity android:name=".SaleLoad.GoodAddLoadActivity" />
        <activity android:name=".SaleLoad.LoadContentActivity"></activity>
    </application>

</manifest>